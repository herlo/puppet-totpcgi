<% @values.each do |val| -%>
<%= @val.encoded_secret %>
<% @val.tokens.each do |token| -%>
<%= token %>
<% end -%>
<% end -%>
<% if @rate_limit -%>
" RATE_LIMIT <%= @rate_limit %>
<% end -%>
<% if @window_size -%>
" WINDOW_SIZE <%= @window_size %>
<% end -%>
<% if @disallow_reuse -%>
" DISALLOW_REUSE
<% end -%>
<% if @totp_auth -%>
" TOTP_AUTH
<%= end -%>
<% if @hotp_counter -%>
" HOTP_COUNTER <%= @hotp_counter %>
<%= end -%>
