<%= @encoded_secret %>
<% if scope.lookupvar('totpcgi::rate_limit') -%>
" RATE_LIMIT <%= scope.lookupvar('totpcgi::rate_limit') %>
<% end -%>
<% if scope.lookupvar('totpcgi::window_size') -%>
" WINDOW_SIZE <%= scope.lookupvar('totpcgi::window_size') %>
<% end -%>
<% if scope.lookupvar('totpcgi::disallow_reuse') -%>
" DISALLOW_REUSE
<% end -%>
<% if scope.lookupvar('totpcgi::totp_auth') -%>
" TOTP_AUTH
<% end -%>
<% if scope.lookupvar('totpcgi::hotp_counter') -%>
" HOTP_COUNTER <%= scope.lookupvar('totpcgi::hotp_counter') %>
<% end -%>
<% @tokens.each do |token| -%>
<%= token %>
<% end -%>

